#include <stdio.h>
#include "Main.h"

/**
* print_to_98-print all natural number
*@n: starting integer
*
*Description: prints all natural numbers
*
*Return: void
*/

void print_to_98(int n)
{
	if (n < 98)
	{
		while (n < 98)
	{
		printf("%d, ", n);
		n++;
	}
	}
	else if (n > 98)
	{
		while (n > 98)
	{
		printf("%d, ", n);
		n--;
	}
	}

	print("98\n");
}
